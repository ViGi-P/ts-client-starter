{
  "name": "ts-client-starter",
  "version": "1.0.0",
  "description": "Frontend starter template using parcel & ts",
  "main": "dist/index.js",
  "repository": "git@github.com:ViGi-P/ts-client-starter.git",
  "homepage": "https://github.com/ViGi-P/ts-client-starter#README",
  "author": "Vignesh Prasad <mail@vigneshprasad.com> (https://vigneshprasad.com/)",
  "license": "MIT",
  "scripts": {
    "start": "./node_modules/.bin/parcel src/index.html -p 3000",
    "build": "rm -rf dist && cp -Rf public dist && ./node_modules/.bin/parcel build src/index.html",
    "docs": "./node_modules/.bin/typedoc"
  },
  "externals": {
    "manifest.json": false
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript"
    ]
  },
  "devDependencies": {
    "@babel/core": "7.11.5",
    "@babel/preset-typescript": "7.10.4",
    "@types/node": "14.6.3",
    "autoprefixer": "9.8.6",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-html-externals": "0.2.0",
    "postcss-modules": "3.2.2",
    "typedoc": "0.19.1",
    "typescript": "^4.0.0"
  }
}
